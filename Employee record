pragma solidity =0.4.21 <0.6.12;

contract Employee{
    string Emp_name;
    uint Emp_id;
    string city;
    uint age;
    uint sallary;
    
    function Employee(string newEmp_name, uint newEmp_id, string newcity, uint newage, uint newsallary)public{
        
        Emp_name = newEmp_name;
        Emp_id = newEmp_id;
        city = newcity;
        age = newage;
        sallary = newsallary;
    }
}
